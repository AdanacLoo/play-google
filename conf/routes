# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

# Google Server
#	First should be blackHole
GET     /blackHole/*path            controllers.Application.blackHole(path: String)

GET     /                           controllers.Application.get(path: String = "/", bbaassee: String ?= "")
GET     /*path                      controllers.Application.get(path: String, bbaassee: String ?= "")
POST    /*path                      controllers.Application.post(path: String)


